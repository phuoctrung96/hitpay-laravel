{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "aws/aws-sdk-php": "~3.0",
        "barryvdh/laravel-cors": "^1.0",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "darkaonline/l5-swagger": "6.x-dev",
        "doctrine/dbal": "^2.3",
        "endroid/qr-code": "^3.7",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^6.5",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^6.2",
        "laravel/horizon": "^3.4",
        "laravel/passport": "^9.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "lcobucci/jwt": "3.3.3",
        "league/csv": "^9.5",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "maatwebsite/excel": "^3.1",
        "moontoast/math": "^1.1",
        "pear/crypt_gpg": "^1.6",
        "pragmarx/google2fa": "^7.0",
        "pragmarx/recovery": "^0.1.0",
        "pusher/pusher-php-server": "^4.1",
        "quickbooks/v3-php-sdk": "^6.0",
        "sentry/sentry-laravel": "^2.11",
        "singpolyma/openpgp-php": "^0.4.0",
        "socialiteproviders/shopify": "^1.0",
        "socialiteproviders/xero": "^3.0",
        "spatie/data-transfer-object": "^2.8",
        "stripe/stripe-php": "^7.14",
        "web-token/jwt-framework": "^2.2",
        "xeroapi/xero-php-oauth2": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "HitPay\\": "hitpay/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "hitpay/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
